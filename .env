# Langflow Configuration
LANGFLOW_PORT=7860

# Langflow PostgreSQL connection string (connects to the Supabase container)
LANGFLOW_DATABASE_URL=postgresql://postgres:${POSTGRES_PASSWORD}@supabase-db:5432/postgres

# Supabase PostgreSQL settings
POSTGRES_PASSWORD=mysecretpassword  # Customize this
POSTGRES_DB=postgres
POSTGRES_USER=postgres

# Supabase API keys (for Kong and Studio if needed)
ANON_KEY=anon-key-value
SERVICE_ROLE_KEY=service-role-key-value
JWT_SECRET=my-jwt-secret
